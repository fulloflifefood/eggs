<div class="grid_12">


  <p style="color: green"><%= flash[:notice] %></p>

  <h2 id="page-heading"><%= "#{@product.name} Subscribers:" %></h2>

  <ul>
    <% @subscriptions.each do |subscription| %>
        <li><%= link_to subscription.account.member.last_name, subscription_transactions_path(:farm_id => @farm.id, :subscription_id => subscription.id) %></li>
    <% end %>
  </ul>

</div>
